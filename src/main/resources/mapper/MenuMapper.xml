<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mhj.dao.MenuDao">

    <select id="findAll" resultType="com.mhj.entity.Menu">
      SELECT id, parent_id AS parentId, name,LEVEL,state from t_menu where is_deleted = 0
    </select>


</mapper>
